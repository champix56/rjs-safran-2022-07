{
  "usesTypeScript": true,
  "usesCssModule": true,
  "cssPreprocessor": "css",
  "testLibrary": "Testing Library",
  "component": {
    "default": {
      "path": "src/App/components",
      "withStyle": true,
      "withTest": true,
      "withStory": true,
      "withLazy": true
    },
    "layout": {
      "path": "src/App/components/layout",
      "withStyle": true,
      "withTest": true,
      "withStory": true,
      "withLazy": false,
      "customTemplates":{
        "component":"grc_templates/layout/TemplateName.tsx"
      }
    },
    "ui": {
      "path": "src/App/components/ui",
      "withStyle": true,
      "withTest": true,
      "withStory": true,
      "withLazy": true,
      "customTemplates":{
        "component":"grc_templates/hookfunction/TemplateName.tsx"
      }
    }
  }
}